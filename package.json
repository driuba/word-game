{
	"$schema": "https://json.schemastore.org/package.json",
	"author": "Andrius Andrikonis",
	"dependencies": {
		"@slack/bolt": "^4.4.0",
		"cron": "^4.3.1",
		"luxon": "^3.7.1",
		"pg": "^8.16.3",
		"reflect-metadata": "^0.2.2",
		"typeorm": "^0.3.25",
		"winston": "^3.17.0"
	},
	"description": "Word game for your slack word game needs",
	"devDependencies": {
		"@eslint/js": "^9.31.0",
		"@slack/cli-hooks": "^1.2.0",
		"@stylistic/eslint-plugin": "^5.1.0",
		"@types/luxon": "^3.6.2",
		"@types/node": "^24.0.13",
		"eslint": "^9.31.0",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.36.0",
		"webpack": "^5.100.1",
		"webpack-cli": "^6.0.1"
	},
	"engines": {
		"node": "^24.4.0",
		"npm": "^11.4.2"
	},
	"keywords": [
		"word-game",
		"word game"
	],
	"license": "GPL-3.0-or-later",
	"main": "dist/app.js",
	"name": "word-game",
	"scripts": {
		"build:development": "webpack build --config webpack.config.mts --mode development",
		"build:production": "webpack build --config webpack.config.mts --mode production",
		"lint": "eslint --flag unstable_native_nodejs_ts_config .",
		"lint:fix": "npm run lint -- --fix",
		"node:development": "node --enable-source-maps --env-file-if-exists .env --env-file-if-exists .env.local --env-file-if-exists .env.development --env-file-if-exists .env.development.local",
		"node:production": "node --enable-source-maps --env-file-if-exists .env --env-file-if-exists .env.local --env-file-if-exists .env.production --env-file-if-exists .env.production.local",
		"start:development": "npm run build:development && npm run node:development ./dist/app.js",
		"start:production": "npm run build:production && npm run node:production ./dist/app.js",
		"typeorm:development": "npm run node:development -- --import ./ts-node/register.ts ./node_modules/typeorm/cli.js --dataSource ./src/entities/index.ts",
		"typeorm:production": "npm run node:production -- --import ./ts-node/register.ts ./node_modules/typeorm/cli.js --dataSource ./src/entities/index.ts"
	},
	"sideEffects": false,
	"type": "module",
	"version": "1.3.3"
}
